{
  "name": "react-nodewebkit",
  "private": true,
  "version": "0.0.1",
  "description": "react node-webkit starter project",
  "keywords": [],
  "window": {
    "icon": "app/images/icon.png",
    "toolbar": true,
    "frame": true,
    "position": "center",
    "width": 1250,
    "height": 800,
    "min_width": 1024,
    "min_height": 768,
    "resizable": true,
    "always-on-top": false,
    "fullscreen": false
  },
  "webkit": {
    "plugin": false
  },
  "main": "_public/index.html",
  "devDependencies": {
    "browserify": "^9.0.3",
    "del": "^1.1.1",
    "es6ify": "^1.6.0",
    "gulp": "^3.8.11",
    "gulp-concat": "^2.5.2",
    "gulp-import-css": "^0.1.2",
    "gulp-sass": "^1.3.3",
    "gutil": "^1.6.1",
    "main-bower-files": "^2.5.0",
    "node-webkit-builder": "^1.0.8",
    "reactify": "^1.0.0",
    "require-dir": "^0.1.0",
    "vinyl-source-stream": "^1.0.0",
    "watchify": "^2.4.0"
  },
  "scripts": {
    "start": "watchify app/jsx/index.jsx -o app/bundle.js -v",
    "app": "nwbuild -r .",
    "build": "./build/build",
    "clean": "rm -rf build/cache app/bundle.js"
  },
  "dependencies": {
    "alt": "^0.15.6",
    "flux": "^2.0.1",
    "keymirror": "^0.1.1",
    "react": "^0.12.2"
  }
}
